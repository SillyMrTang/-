page{
  height:100vh;
  overflow: hidden;
  background-color: #efeff4
}
.header{
  width: 750rpx;
  height: 368rpx;
  background-color: white;
  text-align: center
}
.header image{
  width: 150rpx;
  height: 150rpx;
  border-radius: 50px;
  margin-top: 80rpx
}
.header view{
  margin-top: 15rpx
}
button{
  width: 200rpx;
  font-size: 14px;
  background-color: #1296db;
  color: white
}
.popup-content{
  height: 700rpx;
  background: #ffffff;
  text-align: center;
  position: relative;
  margin-top: 20rpx;
  margin-right: 20rpx;
  margin-left: 20rpx

}
.cancel{
  font-size:32rpx;
  font-weight:400;
  color:rgba(153,153,153,1);
  margin-left: 29rpx
}
.canlendarTopView{
  line-height: 87rpx;
  border-bottom: 1rpx solid #dcdcdc;
  text-align: center
}
.centerView{
  display: flex;

  margin-left: 25%
}
.leftView {
  margin-right: 100rpx
}
.rightView{
  margin-left: 100rpx
}
.leftView image{
  width:15rpx;
  height:24rpx;

}
.rightView image{
  width:15rpx;
  height:24rpx;

}
.yearView{
  font-size:32rpx;
  font-weight:500;
  color:rgba(51,51,51,1);
  margin-bottom: 5rpx;
}
.weekBgView{
  display: flex;
  align-content: center;
  align-items: center;
  margin-top: 45rpx
}
.dateBgView{
    display: flex;
    flex-wrap: wrap;
    align-content: center;
    align-items: center;
}
.dateView{
  display:flex;
  width:100rpx;
  flex-direction:column;
  font-size:32rpx;
  font-weight:400;
  color:rgba(102,102,102,1);
  line-height: 100rpx;
  position: relative

}
.weekView{
  /* flex:1; */
  width: 100rpx;
  text-align:center;
  font-size:32rpx;
  font-weight:500;
  color:rgba(51,51,51,1);

}
.dateSelectView{
  width:78rpx;
  height:78rpx;
  background:rgba(153,215,199,1);
  box-shadow:0px 3rpx 10rpx 0px rgba(153,215,199,0.5);
  border-radius:50%;
  line-height: 78rpx;
  color: white
}
.popup-content{
  background-color: white;
  
}
.today{
  border-radius: 50%;
  background-color: #1296db;
  width: 100rpx;
  height: 100rpx;
  color: white
}
.datesView{
  line-height: 100rpx;
  
}
.isSign{
  border: 1px solid #1296db;
  border-radius: 50%
}

.group-num{
  width:10rpx;
  height:10rpx;
  background:rgba(246,85,68,1);
  border-radius:50%;
  font-weight:400;
  position: absolute;
  top:20rpx;
  right:20rpx
}

